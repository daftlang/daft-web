{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "experimentalDecorators": true,
      "moduleResolution": "node",
      "lib": [
        "esnext",
        "esnext.asynciterable",
        "dom"
      ],
      "esModuleInterop": true,
      "allowJs": true,
      "sourceMap": true,
      "strict": true,
      "noImplicitThis": true,
      "noEmit": true,
      "baseUrl": ".",
      "paths": {
        "~/*": [
          "./*"
        ],
        "@/*": [
          "./*"
        ]
      },
      "typeRoots": [
        "./node_modules/@types",
        "./node_modules/vuetify/types"
      ],
      "types": [
        "@types/node",
        "@nuxt/types",
        "nuxt-i18n",
        "vuetify"
      ]
    },
    "exclude": [
      "node_modules"
    ]
}